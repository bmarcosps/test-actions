{
  "name": "breakout-app",
  "productName": "Breakout App",
  "version": "0.0.10",
  "description": "Breakout local recording application",
  "main": ".webpack/main",
  "author": {
    "name": "App Masters"
  },
  "scripts": {
    "start": "electron-forge start",
    "package": "electron-forge package",
    "make": "electron-forge make",
    "publish": "electron-forge publish",
    "prepare": "husky install",
    "generate": "graphql-codegen --config codegen.yml",
    "test:prettier": "prettier-check \"src/**/*.{js,jsx,ts,tsx}\"",
    "test:lint": "eslint . --ext ts --ext tsx --ext js",
    "test:typescript": "tsc --noEmit",
    "test:package-locks": "package-locks-checks --packageManager=pnpm",
    "checks:linters": "pnpm test:lint && pnpm test:prettier && pnpm test:typescript && pnpm test:package-locks",
    "checks": "pnpm checks:linters && pnpm make",
    "preinstall": "only-allow pnpm"
  },
  "keywords": [],
  "license": "MIT",
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "config": {
    "forge": "./forge.config.js"
  },
  "devDependencies": {
    "@babel/core": "7.18.2",
    "@babel/preset-react": "7.17.12",
    "@electron-forge/cli": "6.0.0-beta.63",
    "@electron-forge/maker-deb": "6.0.0-beta.63",
    "@electron-forge/maker-rpm": "6.0.0-beta.63",
    "@electron-forge/maker-squirrel": "6.0.0-beta.63",
    "@electron-forge/maker-zip": "6.0.0-beta.63",
    "@electron-forge/plugin-webpack": "6.0.0-beta.63",
    "@electron-forge/publisher-s3": "6.0.0-beta.63",
    "@graphql-codegen/cli": "2.6.2",
    "@graphql-codegen/introspection": "2.1.1",
    "@graphql-codegen/typescript": "2.4.11",
    "@graphql-codegen/typescript-operations": "2.4.0",
    "@graphql-codegen/typescript-react-apollo": "3.2.14",
    "@types/bcryptjs": "2.4.2",
    "@types/node": "17.0.40",
    "@types/react": "18.0.12",
    "@types/react-dom": "18.0.5",
    "@types/styled-components": "5.1.25",
    "@typescript-eslint/eslint-plugin": "5.27.1",
    "@typescript-eslint/parser": "5.27.1",
    "@vercel/webpack-asset-relocator-loader": "1.7.2",
    "babel-loader": "8.2.5",
    "css-loader": "6.7.1",
    "dotenv": "16.0.1",
    "electron": "19.0.3",
    "eslint": "8.17.0",
    "eslint-config-prettier": "8.5.0",
    "eslint-plugin-import": "2.26.0",
    "eslint-plugin-prettier": "4.0.0",
    "fork-ts-checker-webpack-plugin": "7.2.11",
    "husky": "8.0.1",
    "node-loader": "2.0.0",
    "only-allow": "1.1.0",
    "prettier": "2.6.2",
    "prettier-check": "2.0.0",
    "style-loader": "3.3.1",
    "ts-loader": "9.3.0",
    "typescript": "4.7.3"
  },
  "dependencies": {
    "@apollo/client": "3.6.6",
    "@babel/runtime": "7.18.3",
    "@types/react-router-dom": "5.3.3",
    "antd": "4.21.0",
    "bcryptjs": "2.4.3",
    "copy-webpack-plugin": "11.0.0",
    "dayjs": "1.11.3",
    "dotenv": "16.0.1",
    "electron-is-dev": "2.0.0",
    "electron-squirrel-startup": "1.0.0",
    "electron-store": "8.0.1",
    "firebase": "9.8.2",
    "graphql": "16.5.0",
    "less": "4.1.2",
    "less-loader": "11.0.0",
    "node-machine-id": "1.1.12",
    "obs-websocket-js": "4.0.3",
    "package-locks-checks": "0.0.8",
    "react": "18.1.0",
    "react-dom": "18.1.0",
    "react-error-boundary": "3.1.4",
    "react-icons": "4.4.0",
    "react-router-dom": "6.3.0",
    "rollbar": "2.25.0",
    "styled-components": "5.3.5"
  },
  "engines": {
    "node": ">=16.15.0"
  }
}
